{
  "name": "proto-syntax-highlight",
  "displayName": "proto-syntax-highlight",
  "description": "Lightweight VS Code syntax highlighting for Protocol Buffer (.proto) files.",
  "repository": "https://github.com/ophirbushi/proto-syntax-highlight",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "OphirBushinsky",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "proto",
        "aliases": [
          "proto",
          "proto"
        ],
        "extensions": [
          ".proto"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "proto",
        "scopeName": "source.proto",
        "path": "./syntaxes/proto.tmLanguage.json"
      }
    ]
  }
}